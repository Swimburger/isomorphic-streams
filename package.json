{
  "name": "@swimburger/isomorphic-streams",
  "version": "1.0.5",
  "description": "Isomorphic WHATWG Streams API package for browser and Node.js. Forked from https://github.com/ksm2/isomorphic-streams",
  "main": "./index.node.cjs",
  "module": "./index.module.mjs",
  "browser": "./index.browser.js",
  "types": "./index.d.ts",
  "exports": {
    "types": "./index.d.ts",
    "node": {
      "import": "./index.node.mjs",
      "require": "./index.node.cjs"
    },
    "default": "./index.browser.js"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Swimburger/isomorphic-streams.git"
  },
  "author": {
    "name": "Niels Swimberghe"
  },
  "bugs": {
    "url": "https://github.com/Swimburger/isomorphic-streams/issues"
  },
  "homepage": "https://github.com/Swimburger/isomorphic-streams",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "keywords": [
    "streams",
    "web-streams",
    "whatwg"
  ],
  "type": "module",
  "engines": {
    "node": ">= 16.5.0"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --no-cache",
    "prettier": "prettier --check *.js *.ts README.md CODE_OF_CONDUCT.md",
    "prettier:fix": "prettier --write *.js *.ts README.md CODE_OF_CONDUCT.md"
  },
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "files": [
    "index.d.ts",
    "index.browser.js",
    "index.node.cjs",
    "index.node.mjs",
    "LICENSE",
    "README.md"
  ]
}